<header class="header-wrp{{#is this.showHeaderTop "true"}} _show-header-top{{/is }}" role="banner">
<div class="header js__header">
    {{#is this.showHeaderTop "true"}}
    <div class="header-top-wrp">
        <div class="header-info js__header-info">
            {{#is this.showHeaderTopContacts "true"}}
            <div class="header-info__contact">
                <div class="header-info__contact-tel">
                    <span class="icons icon-t3-mobile"></span>
                    <span class="header-info__contact-tel-title">Call us on</span>
                    <a class="header-info__contact-tel-link" href="tel:+46-40-617-1230">tel:+46 (40) 617-1230</a>
                </div>
                <div class="header-info__contact-email">
                    <span class="icons icon-t3-mail"></span>
                    <span class="header-info__contact-email-title">Email us at</span>
                    <a class="header-info__contact-email-link" href="#">info@info.se</a>
                </div>
            </div>
            {{/is }}
            {{#is this.showHeaderTopSocIcons "true"}}
            <div class="header-info__social-icons">
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-facebook"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-twitter"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-linkedin"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-vimeo"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-googleplus"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-pinterest"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-instagram"></span></a>
                <a class="header-info__social-icon" href="#"><span class="icons icon-t3-youtube"></span></a>
            </div>
            {{/is }}

            {{#is this.showHeaderTopLangMenu "true"}}
            <div class="header-info__language-menu">
                <a class="header-info__language-menu-btn js__header-info__language-menu-btn" href="/#">
                    <span class="main-flag-icon flag-icon flag-icon-gb"></span>
                </a>
                <div class="header-info__language-menu-box js__header-info__language-menu-box">
                    <a href="" class="header-top__language-menu-box-item active"><span class="flag-icon flag-icon-gb"></span> English</a>
                    <a class="header-top__language-menu-box-item" href=""><span class="flag-icon flag-icon-de"></span> Deutsch</a>
                    <a class="header-top__language-menu-box-item" href=""><span class="flag-icon flag-icon-ro"></span> Română</a>
                    <a class="header-top__language-menu-box-item" href=""><span class="flag-icon flag-icon-se"></span> Svenska</a>
                    <a class="header-top__language-menu-box-item" href=""><span class="flag-icon flag-icon-ua"></span> Українська</a>
                    <a class="header-top__language-menu-box-close-btn js__header-top__language-menu-box-close-btn"></a>
                </div>
            </div>
            {{/is }}

            {{#is this.showHeaderTopNav "true"}}
            <ul class="nav">
                <li><a href="index.html">Home</a></li>
                <li><a href="siteMap.html"> Sitemap</a></li>
                <li><a href="login.html">Login</a></li>
            </ul>
            {{/is }}
        </div>
    </div>
    {{/is }}

    {{ renderContent 'navMinimal'
        dropdown='true'
    }}

</div>
<div class="main-navigation-min__search-box-overlay js__main-navigation-min__search-box-overlay"></div>
<div class="header-info__language-menu-overlay js__header-info__language-menu-overlay"></div>
</header>



